{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "docs": "vue-cli-service build --dest docs",
    "autoformat": "npm run eslint:fix && npm run prettier:fix",
    "eslint:fix": "eslint --ext js,vue --fix .",
    "prettier": "prettier --list-different \"**/*.{js,vue}\"",
    "prettier:fix": "prettier --list-different --write \"**/*.{js,vue}\" ; echo \"All files listed above, if any, have been automatically formatted by Prettier.\""
  },
  "dependencies": {
    "await-to-js": "^3.0.0",
    "axios": "^0.21.1",
    "bootstrap": "^4.4.1",
    "core-js": "^3.6.5",
    "material-design-icons-iconfont": "^6.1.0",
    "node-sass": "^4.13.0",
    "register-service-worker": "^1.7.2",
    "sass-loader": "^7.3.1",
    "vue": "^2.6.11",
    "vue-router": "^3.5.2",
    "vue-select": "^2.6.4",
    "vuetify": "^2.5.8",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
    "@vue/cli-service": "^4.5.0",
    "@vue/eslint-config-prettier": "4.0.1",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "4.0.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-webpack-plugin": "2.5.2",
    "prettier": "^1.16.4",
    "vue-template-compiler": "^2.6.11",
    "babel-preset-airbnb": "3.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint",
      "sourceType": "module"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
